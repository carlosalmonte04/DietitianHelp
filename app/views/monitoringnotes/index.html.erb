<p id="notice"><%= notice %></p>

<h1>Monitoringnotes</h1>

<table>
  <thead>
    <tr>
      <th>Mnote</th>
      <th>Client</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @monitoringnotes.each do |monitoringnote| %>
      <tr>
        <td><%= monitoringnote.mnote %></td>
        <td><%= monitoringnote.client %></td>
        <td><%= link_to 'Show', monitoringnote %></td>
        <td><%= link_to 'Edit', edit_monitoringnote_path(monitoringnote) %></td>
        <td><%= link_to 'Destroy', monitoringnote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Monitoringnote', new_monitoringnote_path %>
