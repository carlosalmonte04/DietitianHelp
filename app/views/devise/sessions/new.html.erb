<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="msapplication-tap-highlight" content="no" />
    <title>DIETITIAN.HELP | Login</title>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons"
      rel="stylesheet">
      <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
      <link href="https://fonts.googleapis.com/css?family=Lato" rel="stylesheet">
      <%= stylesheet_link_tag 'ion.rangeSlider' %>
      <%= stylesheet_link_tag 'selectize' %>
      <%= stylesheet_link_tag 'jquery.dataTables' %>
      <%= stylesheet_link_tag 'selectize' %>
      <%= stylesheet_link_tag 'normalize' %>
      <%= stylesheet_link_tag 'iziToast' %>
      <%= stylesheet_link_tag 'iziModal' %>
      <%= stylesheet_link_tag 'componentPBUTTON' %>
      <%= stylesheet_link_tag 'normalizePBUTTON' %>
      <%= stylesheet_link_tag 'materialLoginForm' %>
      <%= stylesheet_link_tag 'animate.min' %>
      <!--[if IE 8]><script src="js/es5.js"></script><![endif]-->
      <%= favicon_link_tag 'favicon.ico' %>
      <link rel="shortcut icon" href="images/favicon.ico" type="image/x-icon" />
      <!--[if lt IE 9]>
      <script src="html5shiv.min.js"></script>
      <script src="respond.min.js"></script>
      <![endif]-->
    </head>
    <body id="landingBody">
      <%= render partial: 'pages/header' %>
      <div id="fade" class="center">
        <div id="jumbotronnoblur" class="" style="width:100%;height:120%;position: absolute;background-color: #FFFFFFF">
        </div>
          <div id="jumbotronblur" class="" style="width:100%;height:120%;position: absolute;background-image: url('<%= image_path('jumbotron.png')%>">
      </div>
      <img class="animated fadeInUp" id="jumbotronlogo" src="<%= image_path('jumbotronlogo.png')%>" alt="" style="position: absolute;top: 6em;left: 1em;width: 60%;display: none">
      <div id="slogan" style="display: none;">
      <img class="animated fadeInUp" id="sloganpng" src="<%= image_path('slogan.png')%>" alt="" style="position: absolute;display: block;width: 30%;top: 21em;left: 63em">
      </div>
      </div>
      <footer>
        <div><h1>hello!</h1></div>
        <h1>HELLO</h1>
      </footer>
      <!-- Pen Title-->
      <%= javascript_include_tag 'waves.min' %>
      <!-- BLUR BACKGROUND-->
      <%= javascript_include_tag 'jquery.foggy.min' %>
      <script>
      $('.toggle').on('click', function() {
      $('.container').stop().addClass('active');
      });
      $('.close').on('click', function() {
      $('.container').stop().removeClass('active');
      });
      //Waves effect on buttons
      Waves.attach('.waves', ['waves-float']);
      Waves.init();
      $(document).ready(function(){
        $('#fade').hide(0).delay(200).show('fast');
        $('#slogan').delay(1300).fadeIn(580);
        $('#jumbotronlogo').delay(1000).fadeIn(100);
        //$('#jumbotronblur').delay(1000).fadeOut(5000);
        //$('#fade').foggy({
          //blurRadius: 18,          // In pixels.
        //});
        $('.alert').remove();
        $('.notice').remove();
        $('.top-clock').remove();
        $('')
      })
      </script>
    </body>
  </html>