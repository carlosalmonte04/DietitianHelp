<p id="notice"><%= notice %></p>

<h1>Ajaxgets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ajaxgets.each do |ajaxget| %>
      <tr>
        <td><%= link_to 'Show', ajaxget %></td>
        <td><%= link_to 'Edit', edit_ajaxget_path(ajaxget) %></td>
        <td><%= link_to 'Destroy', ajaxget, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ajaxget', new_ajaxget_path %>
