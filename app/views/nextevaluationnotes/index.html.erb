<p id="notice"><%= notice %></p>

<h1>Nextevaluationnotes</h1>

<table>
  <thead>
    <tr>
      <th>Nnote</th>
      <th>Client</th>
      <th>Client</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nextevaluationnotes.each do |nextevaluationnote| %>
      <tr>
        <td><%= nextevaluationnote.nnote %></td>
        <td><%= nextevaluationnote.nnote_date %></td>
        <td><%= nextevaluationnote.client.firstname %></td>
        <td><%= link_to 'Show', nextevaluationnote %></td>
        <td><%= link_to 'Edit', edit_nextevaluationnote_path(nextevaluationnote) %></td>
        <td><%= link_to 'Destroy', nextevaluationnote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

